#include <stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    for(int i=1; i<=n; i++)
    {
        int a,b,c;
        scanf("%d%d%d",&a,&b,&c);
        if(a>b && a>c)
        {
         int f=a;
        }
        else if(b>a && b>c)
        {
            int g=b;
        }
        else if(c>a && c>b)
        {
            int h=c;
        }
        if(a<b && a<c)
        {
         int i=a;
        }
        else if(b<a && b<c)
        {
            int j=b;
        }
        else if(c<a && c<b)
        {
            int k=c;
        }
        printf("%d\n",c-a);
        
    }
}
